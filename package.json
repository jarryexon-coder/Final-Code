  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "server": "node server.js",
    "test": "node test-server.js",
    "test:endpoints": "curl http://localhost:3000/api/nba/games/today && curl http://localhost:3000/api/nba/fantasy/advice",
    "test:phase4": "node test-phase4.js",
    "test:phase5": "node test-phase5.js",
    "test:social": "node test-social.js",
    "test:handlers": "node test-handlers.js",
    "test:health": "node test-health.js",
    "test:teams": "node test-teams.js",
    "test:all": "npm run test:health && npm run test:phase4 && npm run test:phase5 && npm run test:social",
    "test:critical": "curl -s http://localhost:3000/health && curl -s http://localhost:3000/api/nba/games/today | jq .",
    "setup:env": "node setup-env.js",
    "security:reset": "node security-reset.js",
    "heroku:deploy": "git add . && git commit -m 'Deploy to Heroku' && git push heroku main",
    "gae:deploy": "gcloud app deploy --quiet",
    "postinstall": "echo 'ðŸ€ NBA Fantasy AI Backend v4.1 installed successfully!'"
